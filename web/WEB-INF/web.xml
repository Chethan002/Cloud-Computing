<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	 xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	 version="3.1">
    
    <servlet>
        <servlet-name>DO_Reg</servlet-name>
        <servlet-class>ERLS.DO_register</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DO_Reg</servlet-name>
        <url-pattern>/do_reg</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>DU_Reg</servlet-name>
        <servlet-class>ERLS.DU_register</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DU_Reg</servlet-name>
        <url-pattern>/du_reg</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>Cloud</servlet-name>
        <servlet-class>ERLS.Cloud</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Cloud</servlet-name>
        <url-pattern>/cloud</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>KGC</servlet-name>
        <servlet-class>ERLS.KGC</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>KGC</servlet-name>
        <url-pattern>/kgc</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>Data_upload</servlet-name>
        <servlet-class>ERLS.data_upload</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Data_upload</servlet-name>
        <url-pattern>/upload</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>Download_file</servlet-name>
        <servlet-class>ERLS.Download</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Download_file</servlet-name>
        <url-pattern>/download</url-pattern>
    </servlet-mapping>
    
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
